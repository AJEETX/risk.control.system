    {
        <partial name="AdminLTE/_CompanySidebarAdminManageCompany" />
        <partial name="AdminLTE/_CompanySidebarAdminManageAgency" />
        <partial name="AdminLTE/_CompanySidebarAdminManageCompanySettings" />
        <partial name="AdminLTE/_CompanySidebarAdminManageReportTemplate" />
        <partial name="AdminLTE/_CompanySidebarAdminManageGeneralSetup" />
    }
}