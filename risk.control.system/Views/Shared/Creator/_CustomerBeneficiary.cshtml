@model ClaimTransactionModel

@{
    bool customerExist = false;
    bool beneficiaryExist = false;

    if (Model.ClaimsInvestigation.CustomerDetail != null)
    {
        customerExist = true;
    }
    if (Model.Location?.BeneficiaryDetailId > 0)
    {
        beneficiaryExist = true;
    }

    if (@customerExist)
    {
        <partial name="Creator/_FilledCustomer" model="@Model" />
    }
    else
    {
        <partial name="Creator/_BlankCustomer" model="@Model" />

    }

    if (@beneficiaryExist)
    {
        <partial name="Creator/_FilledBeneficiary" model="@Model" />
    }
    else
    {
        <partial name="Creator/_BlankBeneficiary" model="@Model" />
    }
}