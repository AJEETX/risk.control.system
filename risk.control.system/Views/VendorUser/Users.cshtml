@{
    ViewData["Title"] = @Applicationsettings.USER;
}

<section class="content">
    <div class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title"><i class="fas fa-user-friends"></i> Manage Users</h3>
        </div>
        <div class="card-body">
            <input type="hidden" id="vendorId" value="@ViewData["vendorId"]" />
            <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th><i class="fas fa-clock" title="Online status" data-toggle="tooltip"></i>User</th>
                        <th>Email</th>
                        <th>Name</th>
                        <th>Phone</th>
                        <th>Address</th>
                        <th>District</th>
                        <th>State</th>
                        <th>Country</th>
                        <th>Pincode</th>
                        <th>Status</th>
                        <th>Role</th>
                        <th>Actions</th>
                    </tr>
                </thead>

            </table>
        </div>
        <div class="card-footer">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group text-right">
                        <a id="back" asp-controller="Vendors" asp-action="Details" asp-route-id="@ViewData["vendorId"]" class="btn btn-default"><i class="fas fa-arrow-left"></i> @Applicationsettings.BACK</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts    
{
        <script src="~/js/common/company-agency-users.js"></script>
}