@model ClaimsInvestigationVendorsModel


<article>
    <div class="bb-blog-inner">
 
        <div class="card card-light">
            <div class="card-header">
                <h3 class="card-title"><i class="fas fa-print nav-icon"></i> Agency Report: <span class="badge badge-light">@Model.Location.Vendor.Email</span> </h3>

                <div class="card-tools">
                    <span data-toggle="tooltip" title="Agency report # : @Model.Location.Vendor.Email" class="badge badge-light"><i class="fas fa-print nav-icon"></i></span>
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                </div>
            </div>
            
            <div class="card-body">

                <div class="row">
                    <partial name="_AgencyDigitalReport" model="@Model" />
                    <partial name="_AgencyDocumentReport" model="@Model" />
                    <partial name="_AgencyAgentAnswer" model="@Model" />
                    <partial name="_AgencySupervisorAnswer" model="@Model" />
                </div>
            </div>
        </div>
    </div>
</article>