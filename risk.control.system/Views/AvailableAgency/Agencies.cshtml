@{
	ViewData["Title"] = "Available Agencies";
}

<section class="content">
	<div id="article" class="card shadow-lg border-1 rounded-lg">
		<div class="card-header bg-secondary text-white text-center">
			<h3 class="card-title"><i class="fas fa-hand-pointer"></i> Available Agencies</h3>
		</div>
		<form id="checkboxes" asp-action="Agencies" method="post" enctype="multipart/form-data">
			@Html.AntiForgeryToken()
			<div class="card-body">
				<div class="submit-progress hidden">
					<i class="fas fa-sync fa-spin fa-4x fa-fw"></i>
				</div>
				<table id="dataTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
					<thead>
						<tr>
							<th><input type="checkbox" name="select_all" id="checkall" value="0" title="select all to empanel" data-bs-toggle="tooltip" /></th>
							<th>
								Id
							</th>
							<th>
								#
							</th>
							<th>
								Domain
							</th>

							<th>
								Mobile #
							</th>
							<th>
								Address
							</th>
							<th>
								District
							</th>
							<th>
								State
							</th>
							<th>
								Country
							</th>
							<th>
								Updated
							</th>
							<th>
								UpdatedBy
							</th>
							<th>Actions</th>
						</tr>
					</thead>
				</table>
			</div>
			<div class="card-footer">
				<div class="col-md-12">
					<a id="back" asp-controller="Dashboard" asp-action="Index" class="btn btn-default"><i class="fas fa-arrow-left"></i> Back</a>
					<button id="depanel-vendors" disabled type="submit" class="btn btn-success">
						<i class="fas fa-handshake"></i> Empanel
					</button>
				</div>
			</div>
		</form>
	</div>
</section>
@section Scripts {
	<script src="~/js/manager/available-agencies.js"></script>
}