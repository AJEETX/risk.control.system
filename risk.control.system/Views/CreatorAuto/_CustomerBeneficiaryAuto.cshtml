 @model ClaimTransactionModel

@{
    bool customerExist = false;
    bool beneficiaryExist = false;

    if (Model.ClaimsInvestigation.CustomerDetail != null)
    {
        customerExist = true;
    }
    if (Model.Location?.BeneficiaryDetailId > 0)
    {
        beneficiaryExist = true;
    }
 
    if (@customerExist)
    {
        <partial name="_CustomerExist" model="@Model" />
    }
    else
    {
        <partial name="_CustomerNotExist" model="@Model" />

    }

    if (@beneficiaryExist)
    {
        <partial name="_CustomerBeneficiaryExist" model="@Model" />
    }
    else
    {
        <partial name="_CustomerBeneficiaryNotExist" model="@Model" />
    }
}