@model ClaimsInvestigationVendorsModel

@{
    ViewData["Title"] = "Claim: Report ";
    
}

<section class="content">
    <div id="article" class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title"><i class="fa fa-binoculars"></i> Submit
                <span class="hover-text">
                    <i class="fas fa-info-circle"></i>
                    <span class="tooltip-text" id="information">
                        <li>
                           <button class='badge badge-info btn-xs'><i class='fa fa-upload'></i> Upload</button> Photo ID Image

                        </li>
                        <li>
                            <button class='badge badge-info btn-xs'><i class='fa fa-upload'></i> <b>Upload</b> </button> PAN card Image

                        </li>
                        <li>
                            <i class='far fa-hand-point-right'></i>  Fill-up <i class='fa fa-comments'></i> Questionnaire

                        </li>
                        <li>
                            <i class='far fa-hand-point-right'></i> Check T&C <input type="checkbox"> checkbox
                        </li>
                        <li>
                             <button class='badge badge-danger btn-xs'><i class='fa fa-binoculars'></i> <b>SUBMIT</b> </button>

                        </li>
                    </span>
                </span>
            </h3>
        </div>
        <partial name="_PolicyDetails" model="@Model" />
        <partial name="_PolicyReport" model="@Model" />
    </div>

</section>
@section Scripts    {
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDYRB1qIx1AyTxGnV5r5IZC3mk4uYV6MFI&callback=initReportMap">
    </script>
    <script src="~/js/map.js"></script>
    <script src="~/js/get-investigate.js"></script>

}