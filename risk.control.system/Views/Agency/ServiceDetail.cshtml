@using risk.control.system.Helpers
@model risk.control.system.Models.VendorInvestigationServiceType

@{
    ViewData["Title"] = Applicationsettings.DETAILS;
}
<section class="content">
    <div class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title"><i class="fas fa-list-ol"></i> Services</h3>
        </div>
        <div class="card-body">
            <div class="submit-progress hidden">
                <i class="fas fa-sync fa-spin fa-4x fa-fw"></i>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <dl class="row">
                        <dt class="col-sm-5">
                            CaseType
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.InsuranceType.GetEnumDisplayName())
                        </dd>
                        <dt class="col-sm-5">
                            Verification Type
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.InvestigationServiceType.Name)
                        </dd>

                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.District)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.District.Name)
                        </dd>

                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.State)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.State.Name)
                        </dd>
                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.Country)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.Country.Name)
                        </dd>

                    </dl>
                </div>
                <div class="col-md-6">
                    <dl class="row">
                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.Price)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.Price)
                        </dd>

                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.Created)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.Created)
                        </dd>
                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.Updated)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.Updated)
                        </dd>
                        <dt class="col-sm-5">
                            @Html.DisplayNameFor(model => model.UpdatedBy)
                        </dt>
                        <dd class="col-sm-5">
                            @Html.DisplayFor(model => model.UpdatedBy)
                        </dd>
                    </dl>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12 text-right">
                    <a id="back" asp-controller="Agency" asp-action="Service" class="btn btn-secondary">
                        <i class="fa fa-window-close"></i>
                        @Applicationsettings.CANCEL
                    </a>
                    <a asp-action="EditService" class="btn btn-warning" asp-route-id="@Model.VendorInvestigationServiceTypeId"><i class="fas fa-list-ol"></i> Services</a>
                </div>
            </div>
        </div>
    </div>
</section>
