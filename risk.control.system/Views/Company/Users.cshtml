@{
    ViewData["Title"] = @Applicationsettings.USER;
}
<section class="content">
    <div id="article" class="card card-secondary d-flex flex-fill">
        <div class="card-header">
            <h3 class="card-title"><i class="fas fa-user-friends"></i> Manage Users</h3>

        </div>
        <div class="card-body">
            <div class="submit-progress hidden">
                <i class="fas fa-sync fa-spin fa-4x fa-fw"></i>
            </div>
            <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                <thead>
                    <tr>
                        <th></th>
                        <th><i class="fas fa-clock" title="Online status" data-toggle="tooltip"></i></th>
                        <th>Photo</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Address</th>
                        <th>State</th>
                        <th>Pincode</th>
                        <th>Active</th>
                        <th>Role</th>
                        <th>Updated</th>
                        <th>Update By</th>
                        <th>Actions</th>

                    </tr>
                </thead>

            </table>
        </div>
        
    </div>
</section>
@section Scripts    {
    <script src="~/js/company-admin/company-users.js"></script>
}