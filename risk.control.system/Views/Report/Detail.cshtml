@model ClaimTransactionModel

@{
    ViewData["Title"] = "Claim: Report ";
}
<style>
    .container {
        width: 100%;
    }

    .progresses {
        display: flex;
        width: 100%;
        margin: 1rem 0;
    }

    .line-grey {
        width: 100%;
        height: 6px;
        background: lightgrey;
    }

    .line-green {
        width: 100%;
        height: 6px;
        background: green;
    }

    .steps {
        display: flex;
        color: #fff;
        font-size: 1rem;
        width: 200px;
        height: 30px;
        align-items: center;
        justify-content: center;
        border-radius: 50%;
    }

    .steps-green {
        background-color: green;
    }

    .steps-grey {
        background-color: lightgray;
    }

    .claim-status {
        margin-bottom: 50px;
        margin-right: -40px;
    }

    .timeline::after {
        background: #dee2e6;
        top: 1.5rem;
        content: '';
        left: 2rem;
        margin: 0;
        position: relative;
        width: 4px;
    }
    .timeline-reporting::before{

    }
</style>
<section class="content pt-2">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div id="article" class="card card-secondary">
                    <div class="card-header">
                        <h3 class="card-title"><i class="fas fa-newspaper"></i> Detail</h3>
                    </div>
                    @{
                        string address = string.Empty;
                        string baddress = string.Empty;
                        int currentAgeinYrs = 0;
                        int beneficaryAge = 0;

                        if (Model.Location.BeneficiaryDateOfBirth != null)
                        {
                            var bage = DateTime.Now.Subtract(Model.Location.BeneficiaryDateOfBirth).TotalDays;
                            beneficaryAge = (int)bage / 365;
                        }
                        if (Model.Claim.CustomerDetail != null)
                        {
                            address = @Model.Claim?.CustomerDetail.Addressline + " " + @Model.Claim?.CustomerDetail.District.Name + " " + @Model.Claim?.CustomerDetail.State.Name + " " + @Model.Claim?.CustomerDetail.Country.Name + " " + @Model.Claim?.CustomerDetail.PinCode.Code;
                            baddress = @Model.Location != null ? @Model.Location?.Addressline + " " +
                            @Model.Location?.District?.Name + " " + @Model.Location?.State?.Name + " " +
                            @Model.Location?.Country?.Name + " " + @Model.Location?.PinCode?.Code : string.Empty;
                            var age = DateTime.Now.Subtract(Model.Claim.CustomerDetail.CustomerDateOfBirth).TotalDays;
                            currentAgeinYrs = (int)age / 365;
                        }
                        string created = "steps-green";
                        string createdLine = "line-green";

                        string assigned = "steps-green";
                        string assignedLine = "line=green";

                        string allocated = "steps-green";
                        string allocatedLine = "line-green";

                        string tasked = "steps-green";
                        string taskedLine = "line-green";

                        string investigated = "steps-green";
                        string investigatedLine = "line-green";

                        string submitted = "steps-green";
                        string submittedLine = "line-green";

                        string approved = "steps-green";
                        string approvedLine = "line-green";
                        string timeTaken = "< 1";

                        if (Model.Log.Count > 0)
                        {
                            var statuses = Model.Log.Select(s => s.InvestigationCaseSubStatus.Name);
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.APPROVED_BY_ASSESSOR))
                            {
                                approved = "steps-grey";
                                approvedLine = "line-grey";
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.SUBMITTED_TO_ASSESSOR))
                            {
                                submitted = "steps-grey";
                                submittedLine = "line-grey";
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.SUBMITTED_TO_SUPERVISOR))
                            {
                                investigated = "steps-grey";
                                investigatedLine = "line-grey"; ;
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.ASSIGNED_TO_AGENT))
                            {
                                tasked = "steps-grey";
                                taskedLine = "line-grey"; ;
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.ALLOCATED_TO_VENDOR))
                            {
                                allocated = "steps-grey";
                                allocatedLine = "line-grey";
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.ASSIGNED_TO_ASSIGNER))
                            {
                                assigned = "steps-grey";
                                assignedLine = "line-grey";
                            }
                            if (!statuses.Contains(CONSTANTS.CASE_STATUS.CASE_SUBSTATUS.CREATED_BY_CREATOR))
                            {
                                created = "steps-grey";
                                createdLine = "line-grey";
                            }
                            var totalTime = @Model.Log.FirstOrDefault().Created.Subtract(Model.Log.LastOrDefault().Created).Days;
                            if (totalTime > 0)
                            {
                                timeTaken = totalTime.ToString();
                            }
                        }

                        <div class="card-body">
                            <article>
                                <input type="hidden" id="policyDetailId" value="@Model.Claim?.PolicyDetail.PolicyDetailId" />
                                <input type="hidden" id="customerDetailId" value="@Model.Claim?.CustomerDetail.CustomerDetailId" />
                                <input type="hidden" id="beneficiaryId" value="@Model.Location?.CaseLocationId" />
                                <input type="hidden" id="claimId" value="@Model.Claim?.ClaimsInvestigationId" />
                                <div class="bb-blog-inner">
                                    <header class="bb-blog-header">
                                        <h5 class="bb-blog-title" itemprop="name"><i class="far fa-file-powerpoint"></i> Policy # : @Model.Claim.PolicyDetail.ContractNumber</h5>
                                    </header>
                                   
                                    <div class="card card-solid">
                                        <div class="card-body pb-0">
                                            <div class="row">
                                                <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                    <div class="card bg-light d-flex flex-fill">
                                                        <div class="card-header text-muted border-bottom-0">
                                                            <i class="far fa-file-powerpoint"></i> Policy Details
                                                        </div>
                                                        <div class="card-body pt-0">
                                                            <div class="row">
                                                                <div class="col-7">
                                                                    <h4 class="lead"><b>@Model.Claim?.PolicyDetail.LineOfBusiness.Name</b></h4>
                                                                    <p class="text-muted text-sm">
                                                                        <b>Claim Type: </b>@Model.Claim?.PolicyDetail.ClaimType
                                                                    </p>
                                                                    <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-money-bill-wave"></i>
                                                                            </span>
                                                                            Amount: <i class="fas fa-rupee-sign"></i>@Model.Claim?.PolicyDetail.SumAssuredValue
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-tools"></i>
                                                                            </span>
                                                                            Service: @Model.Claim?.PolicyDetail.InvestigationServiceType.Name
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-money-check-alt"></i>
                                                                            </span>
                                                                            Budget center: @Model.Claim?.PolicyDetail.CostCentre.Name
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-sun"></i>
                                                                            </span>
                                                                            Reason: @Model.Claim?.PolicyDetail.CaseEnabler.Name;
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="far fa-clock"></i>
                                                                            </span>
                                                                            Issue Date: @Model.Claim?.PolicyDetail.ContractIssueDate.ToString("dd-MMM-yyyy")
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                                <div class="col-5 text-center">
                                                                    @if (Model?.Claim?.PolicyDetail.DocumentImage is not null)
                                                                    {
                                                                        <img id="documentImage" class="form-Image" src="data:image/*;base64,@(Convert.ToBase64String(Model?.Claim.PolicyDetail.DocumentImage))">
                                                                    }
                                                                    else
                                                                    {
                                                                        <img id="documentImage" alt="user-avatar"
                                                                             class="img-circle img-fluid form-Image" src="/img/no-policy.jpg">
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="card-footer">
                                                            <div class="text-right">
                                                                <button id="policy-comments" class="btn btn-sm bg-teal">
                                                                    <i class="fas fa-comments"></i>
                                                                </button>
                                                                <button id="policy-detail" class="btn btn-sm btn-danger">
                                                                    <i class="far fa-file-powerpoint"></i> ...
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                    <div class="card bg-light d-flex flex-fill">
                                                        <div class="card-header text-muted border-bottom-0">
                                                            <i class="fa fa-user-plus"></i> Customer Details
                                                        </div>
                                                        <div class="card-body pt-0">
                                                            <div class="row">
                                                                <div class="col-7">
                                                                    <h4 class="lead"><b>@Model.Claim.CustomerDetail.CustomerName</b></h4>
                                                                    <p class="text-muted text-sm">
                                                                        <b>Occupation: </b> @Model.Claim.CustomerDetail.CustomerOccupation
                                                                    </p>
                                                                    <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-rupee-sign"></i>
                                                                            </span>
                                                                            Income: @Model.Claim.CustomerDetail.CustomerIncome
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-rupee-sign"></i>
                                                                            </span>
                                                                            Age: @currentAgeinYrs
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-lg fa-building"></i>
                                                                            </span>
                                                                            Address: @address
                                                                        </li>

                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-lg fa-phone"></i>
                                                                            </span>
                                                                            Phone #: + @Model.Claim.CustomerDetail.ContactNumber
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                                <div class="col-5 text-center">
                                                                    @if (Model?.Claim.CustomerDetail.ProfilePicture is not null)
                                                                    {
                                                                        <img id="profileImage" class="form-Image" src="data:image/*;base64,@(Convert.ToBase64String(Model?.Claim.CustomerDetail.ProfilePicture))">
                                                                    }
                                                                    else
                                                                    {
                                                                        <img id="profileImage" class="form-Image" src="/img/user.png">
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="card-footer">
                                                            <div class="text-right">
                                                                <button id="customer-comments" class="btn btn-sm bg-teal">
                                                                    <i class="fas fa-comments"></i>
                                                                </button>
                                                                <button id="customer-detail" class="btn btn-sm btn-warning">
                                                                    <i class="fa fa-user-plus"></i> ...
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                    <div class="card bg-light d-flex flex-fill">
                                                        <div class="card-header text-muted border-bottom-0">
                                                            <i class="fas fa-user-tie"></i>  Beneficiary Details
                                                        </div>
                                                        <div class="card-body pt-0">
                                                            <div class="row">
                                                                <div class="col-7">
                                                                    <h4 class="lead"><b>@Model.Location?.BeneficiaryName</b></h4>
                                                                    <p class="text-muted text-sm">
                                                                        <b>Relation: </b> @Model.Location?.BeneficiaryRelation.Name
                                                                    </p>
                                                                    <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-user-clock"></i>
                                                                            </span>
                                                                            Age: @beneficaryAge
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-rupee-sign"></i>
                                                                            </span>
                                                                            Income: @Model.Location?.BeneficiaryIncome
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-lg fa-building"></i>
                                                                            </span>
                                                                            Address: @baddress
                                                                        </li>
                                                                        <li class="small">
                                                                            <span class="fa-li">
                                                                                <i class="fas fa-lg fa-phone"></i>
                                                                            </span>
                                                                            Phone #: + @Model.Location?.BeneficiaryContactNumber
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                                <div class="col-5 text-center">

                                                                    @if (Model?.Location?.ProfilePicture is not null)
                                                                    {
                                                                        <img id="profileImage" alt="user-avatar"
                                                                             class="img-circle img-fluid form-Image" src="data:image/*;base64,@(Convert.ToBase64String(Model?.Location?.ProfilePicture))">
                                                                    }
                                                                    else
                                                                    {
                                                                        <img class="form-Image" id="profileImage" src="/img/user.png">
                                                                    }

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="card-footer">
                                                            <div class="text-right">
                                                                <button id="beneficiary-comments" class="btn btn-sm bg-teal">
                                                                    <i class="fas fa-comments"></i>
                                                                </button>
                                                                <button id="beneficiary-detail" class="btn btn-sm btn-success">
                                                                    <i class="fas fa-user-tie"></i> ...
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>

                                            @{
                                                if (!string.IsNullOrWhiteSpace(Model.Location?.ClaimReport?.AgentRemarks))
                                                {
                                                    <header class="bb-blog-header">
                                                        <h5 class="bb-blog-title" itemprop="name"> @Model.Location.Vendor.Name Report</h5>
                                                    </header>
                                                    <div class="row">
                                                        <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                            <div class="card bg-light d-flex flex-fill">
                                                                <div class="card-header text-muted border-bottom-0">
                                                                    @{
                                                                        string agentRemarksUpdated = null;
                                                                        if (@Model.Location.ClaimReport?.AgentRemarksUpdated != null)
                                                                        {
                                                                            agentRemarksUpdated = Model.Location.ClaimReport?.AgentRemarksUpdated.Value.ToString("dd-MMM-yy HH:mm");
                                                                        }
                                                                    }
                                                                    <i class="fas fa-mobile-alt"></i> Investigation data <span style="float:right;"><i class="nav-icon fas fa-clock"></i> @agentRemarksUpdated</span>
                                                                </div>
                                                                <div class="card-body pt-0">
                                                                    <div class="row">
                                                                        <div class="col-6">
                                                                            <p class="text-muted text-sm">
                                                                                @{
                                                                                    var QR1 = @Model.Location?.ClaimReport?.AgentOcrData ?? "PAN DATA";
                                                                                    var image = @ViewBag.LocationUrl;
                                                                                    string locationDateTime = null;
                                                                                    if (@Model.Location.ClaimReport?.LocationLongLatTime != null)
                                                                                    {
                                                                                        locationDateTime = @Model.Location.ClaimReport?.LocationLongLatTime.Value.ToString("dd-MMM-yy HH:mm");
                                                                                    }
                                                                                    string ocrDateTime = null;
                                                                                    if (@Model.Location.ClaimReport?.OcrLongLatTime != null)
                                                                                    {
                                                                                        ocrDateTime = @Model.Location.ClaimReport?.OcrLongLatTime.Value.ToString("dd-MMM-yy HH:mm");
                                                                                    }
                                                                                    var ocrImage = @ViewBag.OcrLocationUrl;
                                                                                    var locationData = @Model.Location?.ClaimReport?.LocationData ?? "Location DATA";
                                                                                }
                                                                                <span class="badge badge-light"> Beneficiary Photo</span>: <i class="fas fa-pin"></i> <i class="fas fa-clock"></i> @locationDateTime
                                                                                <br />
                                                                                @{
                                                                                    if (Model.Location.ClaimReport?.AgentLocationPicture != null)
                                                                                    {
                                                                                        <img title="location image" id="agentLocationPicture" class="img-fluid investigation-Image" src="data:image/*;base64,@(Convert.ToBase64String(@Model.Location.ClaimReport?.AgentLocationPicture))">
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <img title="location image" id="agentLocationPicture" alt="location image" class="img-fluid investigation-Image" src="/img/no-photo.png">
                                                                                    }
                                                                                }

                                                                            </p>
                                                                            <p class="text-muted text-sm">
                                                                                <span class="badge badge-light"> Photo Location</span>
                                                                                <br />
                                                                                <img class="locationImage img-fluid " src=@image />
                                                                            </p>
                                                                             <p class="text-muted text-sm">
                                                                            <span class="badge badge-light"> Location Data</span>
                                                                                <label class="ocr-data img-fluid "> @locationData</label>
                                                                        </p>
                                                                        </div>
                                                                        <div class="col-6 text-center">
                                                                            <p class="text-muted text-sm">
                                                                                <span class="badge badge-light"> OCR Photo</span>: <i class="fas fa-pin"></i> <i class="fas fa-clock"></i> @ocrDateTime
                                                                            <br />
                                                                            @{
                                                                                if (Model.Location.ClaimReport?.AgentOcrPicture != null)
                                                                                {
                                                                                    <img title="ocr image" id="agentOcrPicture" class="img-fluid investigation-Image" src="data:image/*;base64,@(Convert.ToBase64String(@Model.Location.ClaimReport?.AgentOcrPicture))">
                                                                                }
                                                                                else
                                                                                {
                                                                                        <img title="ocr image" id="agentOcrPicture" alt="pan-image" class="img-fluid investigation-Image" src="/img/no-photo.png">
                                                                                }
                                                                            }
                                                                            </p>
                                                                            <p class="text-muted text-sm">
                                                                            <span class="badge badge-light"> OCR Location</span>
                                                                            <br />
                                                                                <img class="locationImage img-fluid" src=@ocrImage />
                                                                            </p>
                                                                            <p class="text-muted text-sm">
                                                                                <span class="badge badge-light"> OCR Data</span>
                                                                                <br />
                                                                                <label class="ocr-data img-fluid"> @QR1</label>
                                                                            </p>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        @if (!string.IsNullOrWhiteSpace(Model.Location?.ClaimReport?.AgentRemarks))
                                                        {
                                                            <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                                <div class="card bg-light d-flex flex-fill">
                                                                    <div class="card-header text-muted border-bottom-0">
                                                                        <i class="fas fa-user-shield"></i> Agent  <span style="float:right;"><i class="nav-icon fas fa-clock"></i> @Model.Location.ClaimReport?.AgentRemarksUpdated.Value.ToString("dd-MMM-yy HH:mm")</span>
                                                                    </div>
                                                                    <div class="card-body pt-0">
                                                                        <div class="row">
                                                                            <div class="col-12">
                                                                                <h4 class="lead"><b> @Model?.Location?.ClaimReport.AgentEmail</b></h4>
                                                                                <p class="text-muted text-sm">

                                                                                    <b>Agent report: </b>
                                                                                </p>
                                                                                <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                                    <li class="small">
                                                                                        What is customer age?
                                                                                        <span class="fa-li">
                                                                                            <i class="far fa-comment"></i>
                                                                                        </span>
                                                                                        @Model.Location.ClaimReport?.Question1 <i class="far fa-comment"></i>
                                                                                    </li>
                                                                                    <li class="small">
                                                                                        What is beneficiary relation?
                                                                                        <span class="fa-li">
                                                                                            <i class="far fa-comment"></i>
                                                                                        </span>
                                                                                        @Model.Location.ClaimReport?.Question2 <i class="far fa-comment"></i>
                                                                                    </li>
                                                                                    <li class="small">
                                                                                        Remarks?
                                                                                        <span class="fa-li">
                                                                                            <i class="far fa-comment"></i>
                                                                                        </span>
                                                                                        @Model.Location.ClaimReport?.AgentRemarks <i class="far fa-comment"></i>
                                                                                    </li>
                                                                                </ul>
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        }
                                                        @if (!string.IsNullOrWhiteSpace(Model.Location?.ClaimReport?.SupervisorRemarks))
                                                        {
                                                            <div class="col-12 col-sm-6 col-md-4 d-flex align-items-stretch flex-column">
                                                                <div class="card bg-light d-flex flex-fill">
                                                                    <div class="card-header text-muted border-bottom-0">
                                                                        <i class="fas fa-user-edit"></i> Supervisor <span style="float:right;"><i class="nav-icon fas fa-clock"></i> @Model.Location.ClaimReport?.SupervisorRemarksUpdated.Value.ToString("dd-MMM-yy HH:mm")</span>
                                                                    </div>
                                                                    <div class="card-body pt-0">
                                                                        <div class="row">
                                                                            <div class="col-12">
                                                                                <h4 class="lead"><b> @Model?.Location?.ClaimReport.SupervisorEmail</b></h4>
                                                                                <p class="text-muted text-sm">

                                                                                    <b>Supervisor comments: </b>
                                                                                </p>
                                                                                <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                                    <li class="small">
                                                                                        <span class="fa-li">
                                                                                            <i class="far fa-comment"></i>
                                                                                        </span>
                                                                                        @Model.Location.ClaimReport?.SupervisorRemarks <i class="far fa-comment"></i>
                                                                                    </li>
                                                                                </ul>
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        }
                                                    </div>

                                                    @if (!string.IsNullOrWhiteSpace(Model.Location?.ClaimReport?.AssessorRemarks))
                                                    {
                                                        <div class="row">
                                                            <div class="col-12 d-flex align-items-stretch flex-column">
                                                                <div class="card bg-light d-flex flex-fill">
                                                                    <div class="card-header text-muted border-bottom-0">
                                                                        <i class="fas fa-user-check"></i> Assessor <span style="float:right;"><i class="nav-icon fas fa-clock"></i> @Model.Location.ClaimReport?.AssessorRemarksUpdated.Value.ToString("dd-MMM-yy HH:mm")</span>
                                                                    </div>
                                                                    <div class="card-body pt-0">
                                                                        <div class="row">
                                                                            <div class="col-12">
                                                                                <h4 class="lead"><b> @Model?.Location?.ClaimReport.AssessorEmail</b></h4>
                                                                                <p class="text-muted text-sm">

                                                                                    <b>Assessor remarks: </b>
                                                                                </p>
                                                                                <ul class="ml-4 mb-0 fa-ul text-muted">
                                                                                    <li class="small">
                                                                                        <span class="fa-li">
                                                                                            <i class="far fa-comment"></i>
                                                                                        </span>
                                                                                        @Model.Location.ClaimReport.AssessorRemarks <i class="far fa-comment"></i>
                                                                                    </li>
                                                                                </ul>
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                }
                                            }
                                        </div>
                                    </div>

                                    <div class="card card-solid">
                                        <div class="card-body pb-0">
                                            <span class="badge badge-light"> Total Time taken (days)</span>: <span class="badge badge-info"> @timeTaken</span>
                                            <div id="timeline-reporting" class="row">
                                                <div class="col-12 d-flex align-items-stretch flex-column">
                                                    <div class="bb-blog-content" itemprop="articleBody">
                                                        <div class="timeline">
                                                            @{
                                                                foreach (var log in Model.Log)
                                                                {
                                                                    <div class="time-label"><span class="bg-red">@log.Created.ToString("dd-MMM-yyyy")</span></div>

                                                                    <div>
                                                                        <i class="nav-icon fas fa-envelope bg-blue"></i>
                                                                        <div class="timeline-item">
                                                                            <span class="time"><i class="nav-icon fas fa-clock"></i> @log.Created.ToString("hh:mm tt")</span>
                                                                            <h3 class="timeline-header"><a href="#">@log.UpdatedBy</a> has actioned the case to: <span class="badge badge-success"> @log.InvestigationCaseStatus.Name : @log.InvestigationCaseSubStatus.Name.</span></h3>
                                                                            <div class="timeline-body">
                                                                                The case is moved to <span class="badge badge-info">@log.CurrentClaimOwner</span>
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                }
                                                            }

                                                            <div><i class="nav-icon fas fa-clock bg-gray"></i></div>
                                                        </div>
                                                    </div> <!-- class bb-blog-content -->

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div> <!-- class bb-blog-inner -->
                            </article>
                            <div class="row">
                                <div class="col-md-12 text-right">
                                    <a asp-asp-controller="Report" asp-action="Index" class="btn btn-default"><i class="fas fa-arrow-left"></i> @Applicationsettings.BACK</a> &nbsp;&nbsp;&nbsp;
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
