@{
    ViewData["Title"] = @Applicationsettings.USER;
}
<section class="content">
    <div id="article" class="card card-secondary">
        <div class="card-header">
            <h3 class="card-title"><i class="fas fa-user-friends"></i> Manage Users</h3>
        </div>
        <div class="card-body">
            <div class="submit-progress hidden">
                <i class="fas fa-sync fa-spin fa-4x fa-fw"></i>
            </div>
            <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Photo</th>
                        <th>Email</th>
                        <th>Name</th>
                        <th>Phone</th>
                        <th>Address</th>
                        <th>District</th>
                        <th>State</th>
                        <th>Country</th>
                        <th>Pincode</th>
                        <th>Active</th>
                        <th>Roles</th>
                        <th>Actions</th>

                    </tr>
                </thead>

            </table>
        </div>
    </div>
</section>
@section Scripts    {
    <script src="~/js/all-users.js"></script>

}