                                                <input id="CanChangePassword" asp-for="CanChangePassword" type="checkbox" data-toggle="toggle" data-on="<i class='fas fa-lock-open'></i> Active " data-off="<i class='fas fa-lock'></i> Lock" data-onstyle="success" data-offstyle="danger" data-width="120">

                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span class="input-group-label">BulkUpload:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fa fa-upload"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>

                                            <span class="checkbox">
                                                <input id="BulkUpload" asp-for="BulkUpload" type="checkbox" data-toggle="toggle" data-on="<i class='fas fa-lock-open'></i> Active " data-off="<i class='fas fa-lock'></i> Lock" data-onstyle="success" data-offstyle="danger" data-width="120">
                                            </span>
                                        </div>
                                    </div>
                                    <div class="form-group">

                                        <span class="input-group-label"> Max Assign-Ready Case [ max 50]:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text" asp-for="TotalToAssignMaxAllowed"><i class="fas fa-coins"></i>  <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input required asp-for="TotalToAssignMaxAllowed" class="form-control remarks" type="text" placeholder="Max Assign-Ready Cases"  min="5" maxlength="50" />
                                            <span asp-validation-for="TotalToAssignMaxAllowed" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">

                                    <div class="form-group">
                                        <span class="input-group-label"> License Type:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text" asp-for="LicenseType"><i class="nav-icon fas fa-file-invoice"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <select required asp-for="LicenseType" class="form-control remarks ipaddress" asp-items="Html.GetEnumSelectList<LicenseType>()">
                                                <option value="">--- SELECT ---</option>
                                            </select>
                                            <span asp-validation-for="LicenseType" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="form-group">

                                        <span class="input-group-label"> Expiry Date:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text" asp-for="ExpiryDate"><i class="nav-icon far fa-calendar-check"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input asp-for="ExpiryDate" id="ExpiryDate" class="form-control remarks date-width" type="date" />
                                            <span asp-validation-for="ExpiryDate" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">

                                        <span class="input-group-label"> Max Allowed Case [ max 50]:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text" asp-for="TotalCreatedClaimAllowed"><i class="fas fa-coins"></i>  <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input required asp-for="TotalCreatedClaimAllowed" class="form-control remarks" type="text" placeholder="Max Allowed Cases" min="5" maxlength="50" />
                                            <span asp-validation-for="TotalCreatedClaimAllowed" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <span class="input-group-label"> Enable AI:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-crosshairs"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>

                                            <span class="checkbox">
                                                <input id="AiEnabled" asp-for="AiEnabled" type="checkbox" data-toggle="toggle" data-on="<i class='fas fa-lock-open'></i> Active " data-off="<i class='fas fa-lock'></i> Lock" data-onstyle="success" data-offstyle="danger" data-width="120">
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span class="input-group-label"> Creator SLA (day(s):</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-clock"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input type="text" id="CreatorSla" asp-for="CreatorSla" class="form-control remarks" placeholder="Creator sla (day(s)" />
                                            <span asp-validation-for="CreatorSla" class="text-danger"></span>
                                        </div>
                                    </div>

                                </div>
                                <div class="col-md-3">

                                    <div class="form-group">
                                        <span class="input-group-label"> Assessor SLA (day(s):</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-clock"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input type="text" id="AssessorSla" asp-for="AssessorSla" class="form-control remarks" placeholder="Assessor Sla (day(s)" />
                                            <span asp-validation-for="AssessorSla" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">

                                    <div class="form-group">
                                        <span class="input-group-label"> Supervisor SLA (day(s)):</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-clock"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input type="text" id="SupervisorSla" asp-for="SupervisorSla" class="form-control remarks" placeholder="Supervisor Sla (day(s)" />
                                            <span asp-validation-for="SupervisorSla" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span class="input-group-label"> Agent SLA (day(s):</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-clock"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>
                                            <input type="text" id="AgentSla" asp-for="AgentSla" class="form-control remarks" placeholder="Agent Sla (day(s)" />
                                            <span asp-validation-for="AgentSla" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">

                                    <div class="form-group">
                                        <span class="input-group-label"> Enable Agent Answer update(Supervisor):</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-crosshairs"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>

                                            <span class="checkbox">
                                                <input id="UpdateAgentAnswer" asp-for="UpdateAgentAnswer" type="checkbox" data-toggle="toggle" data-on="<i class='fas fa-lock-open'></i> Active " data-off="<i class='fas fa-lock'></i> Lock" data-onstyle="success" data-offstyle="danger" data-width="120">
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="form-group">
                                        <span class="input-group-label"> Set Prefilled data:</span>
                                        <div class="input-group mb-3">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text"><i class="fas fa-crosshairs"></i> <i class="fa fa-asterisk asterik-style"></i></span>
                                            </div>

                                            <span class="checkbox">
                                                <input id="HasSampleData" asp-for="HasSampleData" type="checkbox" data-toggle="toggle" data-on="<i class='fas fa-lock-open'></i> Active " data-off="<i class='fas fa-lock'></i> Lock" data-onstyle="success" data-offstyle="danger" data-width="120">
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </article>
            <div class="card-footer">
                <div class="row float-right">
                    <div class="col-md-12">
                        <a id="cancel" asp-controller="ClientCompany" asp-action="Details" asp-route-id="@Model.ClientCompanyId" class="btn btn-secondary">
                            <i class="fa fa-window-close"></i>
                            @Applicationsettings.CANCEL
                        </a>
                        <button id="edit" type="submit" class="btn btn-warning">
                            <i class="fas fa-building"></i>  Save Company
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </form>
</section>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    <script src="~/js/company-admin/edit-company.js"></script>
    <script src="~/js/company-admin/pincode.js"></script>
}
