@model DashboardData;

<div class="row">
    <div class="col">

        <div class="small-box bg-danger">
            <div class="inner">
                <h3 id="new-claims">@Model.FirstBlockCount</h3>
                <p>@Model.FirstBlockName</p>
            </div>
            <div class="icon">
                <i class="fa fa-binoculars"></i>
            </div>
            <a href="@Model.FirstBlockUrl" class="details-page small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>

    <div class="col">

        <div class="small-box bg-secondary">
            <div class="inner">
                <h3 id="active-claims">@Model.SecondBlockCount</h3>
                <p>@Model.SecondBlockName</p>
            </div>
            <div class="icon">
                <i class="fas fa-print"></i>
            </div>
            <a href="@Model.SecondBlockUrl" class="details-page small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
        </div>
    </div>

</div>
