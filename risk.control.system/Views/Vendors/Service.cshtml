@{
    ViewData["Title"] = @Applicationsettings.DETAILS;
}
<section class="content pt-2">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card card-secondary">
                    <div class="card-header">
                        <h3 class="card-title"><i class="fas fa-truck"></i> Manage Service</h3>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12">
                                <div class="card card-light">
                                    <a id="create-agency-service" class="btn btn-success" href='#'><i class="fas fa-truck"></i> @Applicationsettings.CREATE New</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 d-flex align-items-stretch flex-column">
                <div class="card card-light bg-light d-flex flex-fill">
                    <div class="card-header">
                        <h3 class="card-title"><i class="fas fa-truck"></i> <span class="badge badge-light"> Services</span></h3>
                        <div class="card-tools">
                            <span data-toggle="tooltip" title="Services" class="badge badge-light"><i class="fas fa-truck"></i></span>
                            <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                <i class="fas fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="card-body pt-0">
                        <input type="hidden" id="vendorId" value="@ViewData["vendorId"]" />
                        <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>CaseType</th>
                                    <th>ServiceType</th>
                                    <th>Rate</th>
                                    <th>District</th>
                                    <th>State</th>
                                    <th>Country</th>
                                    <th>Pincodes</th>
                                    <th>UpdatedBy</th>
                                    <th></th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                    <div class="card-footer">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <a id="back-button" class="btn btn-default"><i class="fas fa-arrow-left"></i> @Applicationsettings.BACK</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
</section>
@section Scripts    {
    <script src="~/js/agency-all-services.js">

    </script>
}