@{
    ViewData["Title"] = @Applicationsettings.DETAILS;
}
<section class="content">
    <div id="article" class="card shadow-lg border-0 rounded-lg">
        <div class="card-header bg-secondary text-white text-center">
            <h3 class="card-title"><i class="fas fa-truck"></i> Manage Service</h3>

        </div>
        <div class="card-body">
            <div class="submit-progress hidden">
                <i class="fas fa-sync fa-spin fa-4x fa-fw"></i>
            </div>
            <input type="hidden" id="vendorId" value="@ViewData["vendorId"]" />
            <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                <thead>
                    <tr>
                        <th></th>
                        <th>CaseType</th>
                        <th>ServiceType</th>
                        <th>Rate</th>
                        <th>District</th>
                        <th>State</th>
                        <th>Country</th>
                        <th>UpdatedBy</th>
                        <th>Updated</th>
                        <th>Actions</th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="card-footer">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group">
                        <a id="back" asp-controller="Vendors" asp-action="Details" asp-route-id="@ViewData["vendorId"]" class="btn btn-outline-secondary"><i class="fas fa-arrow-left"></i> @Applicationsettings.BACK</a>

                        <a class="create-agency-service btn btn-success" asp-controller="VendorService" asp-action="Create" asp-route-id="@ViewData["vendorId"]"><i class="fas fa-truck"></i> Add Service</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    <script src="~/js/company-admin/agency-all-services.js">

    </script>
}