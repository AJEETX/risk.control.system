@{
    ViewData["Title"] = @Applicationsettings.VENDOR;
}

<section class="content pt-2">
    <div class="container-fluid">
        <div class="card border-secondary card-secondary">
            <div class="card-header">
                <h3 class="card-title"><i class="fa fa-binoculars"></i> All Agencies</h3>
            </div>

            <div class="card-body">
                <table id="customerTable" class="table table-bordered table-hover dt-responsive nowrap w-100">
                    <thead>
                        <tr>
                            <th>
                                Id
                            </th>
                            <th>
                                Document
                            </th>
                            <th>
                                Domain
                            </th>
                            <th>
                                Name
                            </th>
                            <th>
                                Code
                            </th>
                            <th>
                                Phone
                            </th>
                            
                            <th>
                                Address
                            </th>
                            <th>
                                District
                            </th>
                            <th>
                                State
                            </th>
                            <th>
                                Country
                            </th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</section>
